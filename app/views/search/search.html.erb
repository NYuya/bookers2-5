<div class="col-xs-9">
	<table class="table">
		<% if model == 'user' %>
			<% @datas.each do |user| %>
				<tr>
			      <td><%= user.name %></td>
			      <td><%= link_to 'Show', user %></td>
			    </tr>
			<% end %>
		<% else model == 'book'%>
			<% @datas.each do |book| %>
				<tr>
			      <td><%= book.title %></td>
			      <td><%= link_to 'Show', book_path(book.id) %></td>
			    </tr>
			<% end %>
		<% end %>

	</table>
</div>
